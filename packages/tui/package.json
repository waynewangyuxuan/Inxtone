{
  "name": "@inxtone/tui",
  "version": "0.1.0",
  "description": "Terminal UI application using Ink",
  "type": "module",
  "bin": {
    "inxtone": "./dist/cli.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "start": "node ./dist/cli.js"
  },
  "dependencies": {
    "@inxtone/core": "workspace:*",
    "@inxtone/server": "workspace:*",
    "ink": "^5.0.0",
    "react": "^18.3.0",
    "commander": "^12.0.0",
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0"
  }
}
